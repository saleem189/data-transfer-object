{
    "name": "saleem/data-transfer-object",
    "description": "A Simple Data Transfer Object without any dependency and command supported",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Saleem\\DataTransferObject\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Saleem Ayoub",
            "email": "saleemayoub1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saleem\\DataTransferObject\\Providers\\DataTransferObjectProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --provider=\"Saleem\\DataTransferObject\\Providers\\DataTransferObjectProvider\" --tag=stubs",
            "@php artisan vendor:publish --provider=\"Saleem\\DataTransferObject\\Providers\\DataTransferObjectProvider\" --tag=config-file"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider=\"Saleem\\DataTransferObject\\Providers\\DataTransferObjectProvider\" --tag=stubs",
            "@php artisan vendor:publish --provider=\"Saleem\\DataTransferObject\\Providers\\DataTransferObjectProvider\" --tag=config-file"
        ]
    }
}
